{
  "name": "dotstamp_client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.local.babel.js",
    "development": "webpack --config ./webpack.config.dev.babel.js",
    "build": "NODE_ENV=production webpack --config ./webpack.config.prod.babel.js",
    "test": "jest",
    "testWatch": "jest --watch",
    "storybook": "start-storybook -p 9009 -s ./dist -c .storybook",
    "build-storybook": "build-storybook -s ./dist -c .storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "flow": "flow",
    "setup-perf": "docker-compose -f sitespeed/docker-compose.yml up",
    "perf-run": "docker-compose -f sitespeed/docker-compose.yml run sitespeed.io https://www.sitespeed.io/ --graphite.host=graphite --preScript /sitespeed.io/script/login.js",
    "stop-perf": "docker-compose -f stop && docker-compose rm"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/js/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/js/__mocks__/styleMock.js"
    },
    "globals": {
      "IMAGE_PATH": "http://",
      "UPLOAD_PATH": "http://",
      "ENV": "test"
    },
    "setupFiles": [
      "./jest-setup.js"
    ]
  },
